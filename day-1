 public int[] twoSum(int[] nums, int target) {
        HashMap<Integer,Integer> m=new HashMap<>();
        int[] val=new int[2];
        for(int i=0;i<nums.length;i++)
        {
            int result=target-nums[i];
            if(m.containsKey(result))
            {
                val[0]=(m.get(result));
                val[1]=i;
            }
            else
            {
                m.put(nums[i],i);
            }
        }
        return val;
    }
